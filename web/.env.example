# Firebase Configuration (Optional - defaults are hardcoded in src/lib/firebase.ts)
# Only set these if you want to override the default Firebase config
# VITE_FIREBASE_API_KEY=your-api-key
# VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
# VITE_FIREBASE_PROJECT_ID=your-project-id
# VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
# VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
# VITE_FIREBASE_APP_ID=your-app-id
# VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Firebase Functions Configuration (Optional)
# VITE_FUNCTIONS_BASE_URL=http://localhost:5001/your-project/us-central1
# VITE_FUNCTIONS_REGION=us-central1

# Development Settings
# These are automatically set by Vite
# MODE=development
# DEV=true
# PROD=false

